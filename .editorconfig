# Documentation of editorconfig file available at
# http://editorconfig.org/


root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[*.py,*.css,]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true

[*.html,*.js]
indent_style = space
indent_size = 2

[Vagrantfile]
indent_style = space
indent_size = 2
